      <xsd:pattern value="[0-9]*\.?[0-9]+([eE][-+]?[0-9]+)?\s*(m\^3|Ym\^3|Zm\^3|Em\^3|Pm\^3|Tm\^3|Gm\^3|Mm\^3|km\^3|hm\^3|dam\^3|L|dm\^3|cm\^3|mm\^3|\\u03BCm\^3|mum\^3|nm\^3|pm\^3|fm\^3|am\^3|zm\^3|ym\^3|NM\^3|pc\^3|ft\^3|in\^3|ly\^3|mi\^3|yd\^3|fl\.oz\(US\)|fl\.oz\(imp\)|gal\(US\)|gal\(imp\)|pt\(US\)|pt\(imp\)|qt\(US\)|qt\(imp\))"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>


  <!-- ================================================================================================== -->
  <!-- ================================================= TYPES ========================================== -->
  <!-- ================================================================================================== -->

  <xsd:complexType name="ModelIdReferralType">
    <xsd:attribute name="Id" type="xsd:string" />
    <xsd:attribute name="ModelId" type="xsd:string" />
  </xsd:complexType>

  <xsd:simpleType name="ClassNameType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([A-Za-z_0-9$]+\.)*[A-Za-z_0-9$]+"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- Same as ClassNameType, just a different binding. -->
  <xsd:simpleType name="StaticFieldNameType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([A-Za-z_0-9$]+\.)*[A-Za-z_0-9$]+"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="PerLengthType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*\/\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- Allows patterns like BEGIN | END | END-10m | 25m | 50% -->
  <xsd:simpleType name="LengthBeginEndType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="BEGIN|END|END\-(\d+\.?\d*\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft))|(\
){0}(\d+\.?\d*\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft)|(\
){0}((\d(\.\d*)?)|(\d\d(\.\d*)?)|(100(\.\0*)?))\s*\%)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="FractionType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(0?\.\d*|1\.0*|1|0)|((\d(\.\d*)?)|(\d\d(\.\d*)?)|(100(\.\0*)?))\s*\%"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="PositiveDouble">
    <xsd:restriction base="xsd:decimal">
      <xsd:minExclusive value="0" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="VehicleIntensityType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*veh"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="VehicleFrequencyType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*veh\s*\/\s*(ms|s|min|h|day|wk)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="CoordinateType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\(\s*[+-]?\d+\.?\d*\s*,\s*[+-]?\d+\.?\d*(\s*,\s*[+-]?\d+\.?\d*)?\s*\)">
      </xsd:pattern>
      <xsd:whiteSpace value="collapse" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="CoordinateNoSpaceType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\([+-]?\d+\.?\d*,[+-]?\d+\.?\d*(,[+-]?\d+\.?\d*)?\)">
      </xsd:pattern>
      <xsd:whiteSpace value="collapse" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="CoordinateListType">
    <xsd:list itemType="ots:CoordinateNoSpaceType" />
  </xsd:simpleType>

  <xsd:simpleType name="LeftRightType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="L|LEFT|R|RIGHT|CLOCKWISE|COUNTERCLOCKWISE"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DistanceCostType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*\/\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="TimeCostType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*\/\s*(ms|s|min|h|day|wk)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="LaneKeepingType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="KEEPLEFT|KEEPRIGHT|KEEPLANE"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="PriorityType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="PRIORITY|NONE|TURN_ON_RED|YIELD|STOP|ALL_STOP|BUS_STOP"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="GtuPositionType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="FRONT|REAR|REFERENCE"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="StripeType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="SOLID|\||DASHED|\:|BLOCKED|DOUBLE|\|\||RIGHTTOLEFT|\|\:|LEFTTORIGHT|\:\|"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DrivingDirectionType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="FORWARD|BACKWARD|BOTH|NONE"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="GtuDirectionType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="DIR_PLUS|DIR_MINUS"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="InterpolationType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="STEPWISE|LINEAR"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ColorType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(\
{0})WHITE|LIGHT_GRAY|GRAY|DARK_GRAY|BLACK|RED|PINK|ORANGE|YELLOW|GREEN|MAGENTA|CYAN|BLUE(\
{0})|\#[(0-9)|(A-F)]{6}(\
{0})|RGB\(0*([1-9]?\d|1\d\d|2[0-4]\d|25[0-5])\s*\,\s*(\
{0})0*([1-9]?\d|1\d\d|2[0-4]\d|25[0-5])\s*\,\s*(\
{0})0*([1-9]?\d|1\d\d|2[0-4]\d|25[0-5])\)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="InputParameterType">
    <xsd:attribute name="Type" use="required">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:pattern value="(\
{0})CLASS|STRING|DOUBLE|INTEGER|(\
{0})LENGTH|POSITION|DURATION|TIME|SPEED|ACCELERATION|FREQUENCY|LINEARDENSITY|(\
{0})FRACTION" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="Value" type="xsd:string" use="required" />
    <xsd:attribute name="Id" type="xsd:string" use="required" />
    <xsd:attribute name="Class" type="ots:ClassNameType" use="optional" />
  </xsd:complexType>

  <!-- ========================================================================================= -->
  <!-- ==================================== DISTRIBUTION TYPES ================================= -->
  <!-- ========================================================================================= -->

  <xsd:simpleType name="DoublePositiveZero">
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="0.0"></xsd:minInclusive>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DoublePositive">
    <xsd:restriction base="xsd:double">
      <xsd:minExclusive value="0.0"></xsd:minExclusive>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DoubleUnitInterval">
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="0.0"></xsd:minInclusive>
      <xsd:maxInclusive value="1.0"></xsd:maxInclusive>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="ConstantDistType">
    <xsd:choice>
      <xsd:element name="Constant">
        <xsd:complexType>
          <xsd:attribute name="C" type="xsd:double" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Exponential">
        <xsd:complexType>
          <xsd:attribute name="Lambda" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Triangular">
        <xsd:complexType>
          <xsd:attribute name="Min" type="xsd:double" use="required" />
          <xsd:attribute name="Mode" type="xsd:double" use="required" />
          <xsd:attribute name="Max" type="xsd:double" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Normal">
        <xsd:complexType>
          <xsd:attribute name="Mu" type="xsd:double" use="required" />
          <xsd:attribute name="Sigma" type="ots:DoublePositiveZero" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Beta">
        <xsd:complexType>
          <xsd:attribute name="Alpha1" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="Alpha2" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Erlang">
        <xsd:complexType>
          <xsd:attribute name="Mean" type="xsd:double" use="required" />
          <xsd:attribute name="K" type="xsd:positiveInteger" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Gamma">
        <xsd:complexType>
          <xsd:attribute name="Alpha" type="xsd:double" use="required" />
          <xsd:attribute name="Beta" type="xsd:double" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="LogNormal">
        <xsd:complexType>
          <xsd:attribute name="Mu" type="xsd:double" use="required" />
          <xsd:attribute name="Sigma" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Pearson5">
        <xsd:complexType>
          <xsd:attribute name="Alpha" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="Beta" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Pearson6">
        <xsd:complexType>
          <xsd:attribute name="Alpha1" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="Alpha2" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="Beta" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Uniform">
        <xsd:complexType>
          <xsd:attribute name="Min" type="xsd:double" use="required" />
          <xsd:attribute name="Max" type="xsd:double" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Weibull">
        <xsd:complexType>
          <xsd:attribute name="Alpha" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="Beta" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="DiscreteDistType">
    <xsd:choice>
      <xsd:element name="Constant">
        <xsd:complexType>
          <xsd:attribute name="C" type="xsd:long" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="BernoulliI">
        <xsd:complexType>
          <xsd:attribute name="P" type="ots:FractionType" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Binomial">
        <xsd:complexType>
          <xsd:attribute name="N" type="xsd:positiveInteger" use="required" />
          <xsd:attribute name="P" type="ots:FractionType" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Uniform">
        <xsd:complexType>
          <xsd:attribute name="Min" type="xsd:int" use="required" />
          <xsd:attribute name="Max" type="xsd:int" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Geometric">
        <xsd:complexType>
          <xsd:attribute name="P" type="ots:FractionType" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="NegBinomial">
        <xsd:complexType>
          <xsd:attribute name="N" type="xsd:positiveInteger" use="required" />
          <xsd:attribute name="P" type="ots:FractionType" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Poisson">
        <xsd:complexType>
          <xsd:attribute name="Lambda" type="ots:DoublePositive" use="required" />
          <xsd:attribute name="RandomStream" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="LengthDistType">
