<ots:PARAMETERS xmlns:ots="http://www.opentrafficsim.org/ots" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots ../ots-model.xsd">

  <!-- Most common parameters, for LMRS and other models -->
  <ots:DURATION ID="dt" DESCRIPTION="Fixed model time step" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.DT" />
  <ots:LENGTH ID="s0" DESCRIPTION="Car-following stopping distance" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.S0" />
  <ots:ACCELERATION ID="a" DESCRIPTION="Maximum (desired) car-following acceleration" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.A" />
  <!-- b0 < b < bCrit -->
  <ots:ACCELERATION ID="b" DESCRIPTION="Maximum comfortable car-following deceleration" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.B" />
  <ots:ACCELERATION ID="bCrit" DESCRIPTION="Maximum critical deceleration, e.g. stop/go at traffic light" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.BCRIT" />
  <ots:ACCELERATION ID="b0" DESCRIPTION="Maximum adjustment deceleration, e.g. when speed limit drops" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.B0" />
  <ots:DURATION ID="T" DESCRIPTION="Current car-following headway" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.T" />
  <!-- Tmin < Tmax -->
  <ots:DURATION ID="Tmin" DESCRIPTION="Minimum car-following headway" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.TMIN" />
  <ots:DURATION ID="Tmax" DESCRIPTION="Maximum car-following headway" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.TMAX" />
  <ots:DURATION ID="tau" DESCRIPTION="Headway relaxation time" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.TAU" />
  <ots:DURATION ID="t0" DESCRIPTION="Look-ahead time for mandatory lane changes" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.T0" />
  <ots:LENGTH ID="Look-ahead" DESCRIPTION="Look-ahead distance" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.LOOKAHEAD" />
  <ots:LENGTH ID="Look-back" DESCRIPTION="Look-back distance" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.LOOKBACK" />
  <ots:DOUBLE ID="fSpeed" DESCRIPTION="Speed limit adherence factor" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.FSPEED" />
  <ots:SPEED ID="vCong" DESCRIPTION="Speed threshold below which traffic is considered congested" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.VCONG" />
  <ots:DURATION ID="lcDur" DESCRIPTION="Regular lane change duration" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.LCDUR" />
  <ots:LENGTH ID="perception" DESCRIPTION="Mental map length" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.PERCEPTION" />
  <ots:DURATION ID="Tr" DESCRIPTION="Reaction time" FIELD="org.opentrafficsim.base.parameters.ParameterTypes.TR" />
  
  <!-- IDM parameters -->
  <ots:DOUBLE ID="delta" DESCRIPTION="Acceleration flattening exponent towards desired speed" FIELD="org.opentrafficsim.road.gtu.lane.tactical.following.AbstractIDM.DELTA" />
  
  <!-- LMRS parameters -->
  <!-- dFree < dSync < dCoop -->
  <ots:DOUBLE ID="dFree" DESCRIPTION="Free lane change desire threshold" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.lmrs.LmrsParameters.DFREE" />
  <ots:DOUBLE ID="dSync" DESCRIPTION="Synchronized lane change desire threshold" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.lmrs.LmrsParameters.DSYNC" />
  <ots:DOUBLE ID="dCoop" DESCRIPTION="Cooperative lane change desire threshold" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.lmrs.LmrsParameters.DCOOP" />
  <ots:SPEED ID="vGain" DESCRIPTION="Anticipation speed difference at full lane change desire" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.lmrs.LmrsParameters.VGAIN" />
  <ots:DOUBLE ID="courtesy" DESCRIPTION="Courtesy level for courtesy lane changes" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.lmrs.LmrsParameters.COURTESY" />
  <ots:DOUBLE ID="socio" DESCRIPTION="Sensitivity level for speed of others" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.lmrs.LmrsParameters.SOCIO" />
  <ots:DOUBLE ID="rho" DESCRIPTION="Social pressure" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.lmrs.Tailgating.RHO" />
  
  <!-- Perception parameters -->
  <ots:DOUBLE ID="SA" DESCRIPTION="Situational awareness" FIELD="org.opentrafficsim.road.gtu.lane.perception.mental.AdaptationSituationalAwareness.SA" />
  <!-- SAmin < SAmax -->
  <ots:DOUBLE ID="SAmin" DESCRIPTION="Min. situational awareness"
    FIELD="org.opentrafficsim.road.gtu.lane.perception.mental.AdaptationSituationalAwareness.SA_MIN" />
  <ots:DOUBLE ID="SAmax" DESCRIPTION="Max. situational awareness"
    FIELD="org.opentrafficsim.road.gtu.lane.perception.mental.AdaptationSituationalAwareness.SA_MAX" />
  <ots:DURATION ID="TRmax" DESCRIPTION="Maximum reaction time"
    FIELD="org.opentrafficsim.road.gtu.lane.perception.mental.AdaptationSituationalAwareness.TR_MAX" />
  <ots:DOUBLE ID="TC" DESCRIPTION="Task capability" FIELD="org.opentrafficsim.road.gtu.lane.perception.mental.Fuller.TC" />
  <ots:DOUBLE ID="TS" DESCRIPTION="Task saturation" FIELD="org.opentrafficsim.road.gtu.lane.perception.mental.Fuller.TS" />
  <!-- TScrit < TSmax -->
  <ots:DOUBLE ID="TScrit" DESCRIPTION="Critical task saturation" FIELD="org.opentrafficsim.road.gtu.lane.perception.mental.Fuller.TS_CRIT" />
  <ots:DOUBLE ID="TSmax" DESCRIPTION="Maximum task saturation" FIELD="org.opentrafficsim.road.gtu.lane.perception.mental.Fuller.TS_MAX" />
  
  <!-- Intersection parameters -->
  <ots:DURATION ID="minGap" DESCRIPTION="Minimum gap for conflicts" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.ConflictUtil.MIN_GAP" />
  <ots:LENGTH ID="s0conf" DESCRIPTION="Stopping distance at conflicts" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.ConflictUtil.S0_CONF" />
  <ots:DOUBLE ID="timeFactor" DESCRIPTION="Safety factor on estimated time" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.ConflictUtil.TIME_FACTOR" />
  <ots:DOUBLE ID="stopArea" DESCRIPTION="Area before stop line where one is considered arrived at the intersection" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.ConflictUtil.STOP_AREA" />
  <ots:DOUBLE ID="ti" DESCRIPTION="Indicator time before bus departure" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.ConflictUtil.TI" />
  <ots:ACCELERATION ID="bYellow" DESCRIPTION="Maximum deceleration for stopping for yellow traffic light" FIELD="org.opentrafficsim.road.gtu.lane.tactical.util.TrafficLightUtil.B_YELLOW" />

  <!-- (C)ACC parameters -->
  <ots:DURATION ID="td ACC" DESCRIPTION="User defined time headway in ACC mode" FIELD="org.opentrafficsim.road.gtu.lane.control.AbstractActuatedControl.TDACC" />
  <ots:DURATION ID="td CACC" DESCRIPTION="User defined time headway in CACC mode" FIELD="org.opentrafficsim.road.gtu.lane.control.AbstractActuatedControl.TDCACC" />
  <ots:LENGTH ID="x0 (C)ACC" DESCRIPTION="Stopping distance (C)ACC" FIELD="org.opentrafficsim.road.gtu.lane.control.AbstractActuatedControl.X0" />
  <ots:DOUBLE ID="kf" DESCRIPTION="Desired speed error gain" FIELD="org.opentrafficsim.road.gtu.lane.control.AbstractLinearFreeControl.KF" />
  <ots:DOUBLE ID="ks" DESCRIPTION="Gap error gain" FIELD="org.opentrafficsim.road.gtu.lane.control.LinearCACC.KS" />
  <ots:DOUBLE ID="kv" DESCRIPTION="Speed error gain" FIELD="org.opentrafficsim.road.gtu.lane.control.LinearCACC.KV" />
  <ots:DOUBLE ID="ka" DESCRIPTION="Acceleration error gain" FIELD="org.opentrafficsim.road.gtu.lane.control.LinearCACC.KA" />
  <ots:DOUBLE ID="kd" DESCRIPTION="Gap error derivative gain" FIELD="org.opentrafficsim.road.gtu.lane.control.PloegCACC.KD" />

</ots:PARAMETERS>