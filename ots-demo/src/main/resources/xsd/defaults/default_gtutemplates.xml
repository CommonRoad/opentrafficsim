<ots:GTUTEMPLATES xmlns:ots="http://www.opentrafficsim.org/ots"  
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots ../ots-definitions.xsd">

