              new ConstantGenerator<>(Length.createSI(1.7)),
              new ConstantGenerator<>(new Speed(180, SpeedUnit.KM_PER_HOUR)));

      // GTUType.TRUCK
      // from "Maatgevende normen in de Nederlandse richtlijnen voor wegontwerp", R-2014-38, SWOV
      template = new TemplateGTUType(type, new ConstantGenerator<>(Length.createSI(12.0)),
              new ConstantGenerator<>(Length.createSI(2.55)),
              new ContinuousDistSpeed(new DistNormal(randomStream, 85.0, 2.5), SpeedUnit.KM_PER_HOUR));
            
      // GTUType.BUS
      template = new TemplateGTUType(type, new ConstantGenerator<>(Length.createSI(12.0)),
              new ConstantGenerator<>(Length.createSI(2.55)),
              new ConstantGenerator<>(new Speed(90, SpeedUnit.KM_PER_HOUR)));

      // GTUType.VAN
      template = new TemplateGTUType(type, new ConstantGenerator<>(Length.createSI(5.0)),
              new ConstantGenerator<>(Length.createSI(2.4)),
              new ConstantGenerator<>(new Speed(180, SpeedUnit.KM_PER_HOUR)));

      // EMERGENCY_VEHICLE
      template = new TemplateGTUType(type, new ConstantGenerator<>(Length.createSI(5.0)),
              new ConstantGenerator<>(Length.createSI(2.55)),
              new ConstantGenerator<>(new Speed(180, SpeedUnit.KM_PER_HOUR)));
 -->

  <ots:GTUTEMPLATE ID="CAR" GTUTYPE="CAR" DEFAULT="true">
    <ots:LENGTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="4.19" />
    </ots:LENGTHDIST>
    <ots:WIDTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="1.7" />
    </ots:WIDTHDIST>
    <ots:MAXSPEEDDIST SPEEDUNIT="km/h">
      <ots:CONSTANT C="180" />
    </ots:MAXSPEEDDIST>
  </ots:GTUTEMPLATE>

  <ots:GTUTEMPLATE ID="TRUCK" GTUTYPE="TRUCK" DEFAULT="true">
    <ots:LENGTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="12.0" />
    </ots:LENGTHDIST>
    <ots:WIDTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="2.55" />
    </ots:WIDTHDIST>
    <ots:MAXSPEEDDIST SPEEDUNIT="km/h">
      <ots:NORMAL MU="85.0" SIGMA="2.5" />
    </ots:MAXSPEEDDIST>
  </ots:GTUTEMPLATE>

  <ots:GTUTEMPLATE ID="BUS" GTUTYPE="BUS" DEFAULT="true">
    <ots:LENGTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="12.0" />
    </ots:LENGTHDIST>
    <ots:WIDTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="2.55" />
    </ots:WIDTHDIST>
    <ots:MAXSPEEDDIST SPEEDUNIT="km/h">
      <ots:CONSTANT C="90" />
    </ots:MAXSPEEDDIST>
  </ots:GTUTEMPLATE>

  <ots:GTUTEMPLATE ID="VAN" GTUTYPE="VAN" DEFAULT="true">
    <ots:LENGTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="5.0" />
    </ots:LENGTHDIST>
    <ots:WIDTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="2.4" />
    </ots:WIDTHDIST>
    <ots:MAXSPEEDDIST SPEEDUNIT="km/h">
      <ots:CONSTANT C="180" />
    </ots:MAXSPEEDDIST>
  </ots:GTUTEMPLATE>

  <ots:GTUTEMPLATE ID="EMERGENCY_VEHICLE" GTUTYPE="EMERGENCY_VEHICLE" DEFAULT="true">
    <ots:LENGTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="5.0" />
    </ots:LENGTHDIST>
    <ots:WIDTHDIST LENGTHUNIT="m">
      <ots:CONSTANT C="2.4" />
    </ots:WIDTHDIST>
    <ots:MAXSPEEDDIST SPEEDUNIT="km/h">
      <ots:CONSTANT C="180" />
    </ots:MAXSPEEDDIST>
  </ots:GTUTEMPLATE>

</ots:GTUTEMPLATES>
