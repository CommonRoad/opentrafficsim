<?xml version="1.0" encoding="UTF-8" ?>
<ots:OTS xmlns:ots="http://www.opentrafficsim.org/ots" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.opentrafficsim.org/ots ../../../../../ots-parser-xml/src/main/resources/xsd/ots.xsd"
  xmlns:xi="http://www.w3.org/2001/XInclude">

  <ots:DEFINITIONS>
  
    <xi:include href="../xsd/defaults/default_gtutypes.xml">
      <xi:fallback>
        <xi:include href="../../../../../ots-parser-xml/src/main/resources/xsd/defaults/default_gtutypes.xml" />
      </xi:fallback>
    </xi:include>
    <xi:include href="../xsd/defaults/default_linktypes.xml">
      <xi:fallback>
        <xi:include href="../../../../../ots-parser-xml/src/main/resources/xsd/defaults/default_linktypes.xml" />
      </xi:fallback>
    </xi:include>
    <xi:include href="../xsd/defaults/default_lanetypes.xml">
      <xi:fallback>
        <xi:include href="../../../../../ots-parser-xml/src/main/resources/xsd/defaults/default_lanetypes.xml" />
      </xi:fallback>
    </xi:include>

    <ots:LINKTYPES>
      <ots:LINKTYPE ID="HIGHWAY">
        <ots:COMPATIBILITY GTUTYPE="VEHICLE" DIRECTION="FORWARD" />
        <ots:SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="120km/h" />
        <ots:SPEEDLIMIT GTUTYPE="TRUCK" LEGALSPEEDLIMIT="90km/h" />
      </ots:LINKTYPE>
      <ots:LINKTYPE ID="LOCAL">
        <ots:COMPATIBILITY GTUTYPE="VEHICLE" DIRECTION="FORWARD" />
        <ots:SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="20km/h" />
        <ots:SPEEDLIMIT GTUTYPE="TRUCK" LEGALSPEEDLIMIT="20km/h" />
      </ots:LINKTYPE>
    </ots:LINKTYPES>

    <ots:ROADLAYOUTS>

      <ots:ROADLAYOUT ID="3LaneHighway" LINKTYPE="HIGHWAY">
        <ots:SHOULDER>
          <ots:CENTEROFFSET>12.1m</ots:CENTEROFFSET>
          <ots:WIDTH>3m</ots:WIDTH>
        </ots:SHOULDER>
        <ots:STRIPE TYPE="SOLID">
          <ots:CENTEROFFSET>10.8m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:LANE ID="FORWARD1" DESIGNDIRECTION="true" LANETYPE="HIGHWAY">
          <ots:CENTEROFFSET>9.0m</ots:CENTEROFFSET>
          <ots:WIDTH>3.6m</ots:WIDTH>
        </ots:LANE>
        <ots:STRIPE TYPE="DASHED">
          <ots:CENTEROFFSET>7.2m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:LANE ID="FORWARD2" DESIGNDIRECTION="true" LANETYPE="HIGHWAY">
          <ots:CENTEROFFSET>5.4m</ots:CENTEROFFSET>
          <ots:WIDTH>3.6m</ots:WIDTH>
        </ots:LANE>
        <ots:STRIPE TYPE="DASHED">
          <ots:CENTEROFFSET>3.6m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:LANE ID="FORWARD3" DESIGNDIRECTION="true" LANETYPE="HIGHWAY">
          <ots:CENTEROFFSET>1.8m</ots:CENTEROFFSET>
          <ots:WIDTH>3.6m</ots:WIDTH>
        </ots:LANE>
        <ots:STRIPE TYPE="SOLID">
          <ots:CENTEROFFSET>0m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:SHOULDER>
          <ots:CENTEROFFSET>-1.5m</ots:CENTEROFFSET>
          <ots:WIDTH>3m</ots:WIDTH>
        </ots:SHOULDER>
      </ots:ROADLAYOUT>

      <ots:ROADLAYOUT ID="2LaneHighway" LINKTYPE="HIGHWAY">
        <ots:SHOULDER>
          <ots:CENTEROFFSET>8.5m</ots:CENTEROFFSET>
          <ots:WIDTH>3m</ots:WIDTH>
        </ots:SHOULDER>
        <ots:STRIPE TYPE="SOLID">
          <ots:CENTEROFFSET>7.2m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:LANE ID="FORWARD1" DESIGNDIRECTION="true" LANETYPE="HIGHWAY">
          <ots:CENTEROFFSET>5.4m</ots:CENTEROFFSET>
          <ots:WIDTH>3.6m</ots:WIDTH>
        </ots:LANE>
        <ots:STRIPE TYPE="DASHED">
          <ots:CENTEROFFSET>3.6m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:LANE ID="FORWARD2" DESIGNDIRECTION="true" LANETYPE="HIGHWAY">
          <ots:CENTEROFFSET>1.8m</ots:CENTEROFFSET>
          <ots:WIDTH>3.6m</ots:WIDTH>
        </ots:LANE>
        <ots:STRIPE TYPE="SOLID">
          <ots:CENTEROFFSET>0m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:SHOULDER>
          <ots:CENTEROFFSET>-1.5m</ots:CENTEROFFSET>
          <ots:WIDTH>3m</ots:WIDTH>
        </ots:SHOULDER>
      </ots:ROADLAYOUT>

      <ots:ROADLAYOUT ID="1LaneHighway" LINKTYPE="HIGHWAY">
        <ots:SHOULDER>
          <ots:CENTEROFFSET>4.9m</ots:CENTEROFFSET>
          <ots:WIDTH>3m</ots:WIDTH>
        </ots:SHOULDER>
        <ots:STRIPE TYPE="SOLID">
          <ots:CENTEROFFSET>3.6m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:LANE ID="FORWARD1" DESIGNDIRECTION="true" LANETYPE="HIGHWAY">
          <ots:CENTEROFFSET>1.8m</ots:CENTEROFFSET>
          <ots:WIDTH>3.6m</ots:WIDTH>
        </ots:LANE>
        <ots:STRIPE TYPE="SOLID">
          <ots:CENTEROFFSET>0m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:SHOULDER>
          <ots:CENTEROFFSET>-1.5m</ots:CENTEROFFSET>
          <ots:WIDTH>3m</ots:WIDTH>
        </ots:SHOULDER>
      </ots:ROADLAYOUT>

      <ots:ROADLAYOUT ID="1LaneLocal" LINKTYPE="LOCAL">
        <ots:SHOULDER>
          <ots:CENTEROFFSET>4.9m</ots:CENTEROFFSET>
          <ots:WIDTH>3m</ots:WIDTH>
        </ots:SHOULDER>
        <ots:STRIPE TYPE="SOLID">
          <ots:CENTEROFFSET>3.6m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:LANE ID="FORWARD1" DESIGNDIRECTION="true" LANETYPE="HIGHWAY">
          <ots:CENTEROFFSET>1.8m</ots:CENTEROFFSET>
          <ots:WIDTH>3.6m</ots:WIDTH>
        </ots:LANE>
        <ots:STRIPE TYPE="SOLID">
          <ots:CENTEROFFSET>0m</ots:CENTEROFFSET>
          <ots:DRAWINGWIDTH>20cm</ots:DRAWINGWIDTH>
        </ots:STRIPE>
        <ots:SHOULDER>
          <ots:CENTEROFFSET>-1.5m</ots:CENTEROFFSET>
          <ots:WIDTH>3m</ots:WIDTH>
        </ots:SHOULDER>
      </ots:ROADLAYOUT>

    </ots:ROADLAYOUTS>
  </ots:DEFINITIONS>

  <ots:NETWORK>
    <ots:NODE ID="A" COORDINATE="(0, 0)" DIRECTION="0 deg(E)" />
    <ots:NODE ID="B" COORDINATE="(1500, 0)" DIRECTION="0 deg(E)" />
    <ots:NODE ID="C" COORDINATE="(1565, 0)" DIRECTION="0 deg(E)" />
    <ots:NODE ID="D" COORDINATE="(1570, 0)" DIRECTION="0 deg(E)" />
    <ots:NODE ID="E" COORDINATE="(3000, 0)" DIRECTION="0 deg(E)" />
    <ots:NODE ID="F" COORDINATE="(1260, -40)" DIRECTION="0 deg(E)" />
    <ots:NODE ID="F2" COORDINATE="(1280, -40)" DIRECTION="0 deg(E)" />

    <ots:LINK ID="AB" NODESTART="A" NODEEND="B" TYPE="HIGHWAY">
      <ots:STRAIGHT />
      <ots:DEFINEDLAYOUT>2LaneHighway</ots:DEFINEDLAYOUT>
    </ots:LINK>

    <ots:LINK ID="BC" NODESTART="B" NODEEND="C" TYPE="HIGHWAY" OFFSETSTART="-3.6m" OFFSETEND="-3.6m">
      <ots:STRAIGHT />
      <ots:DEFINEDLAYOUT>3LaneHighway</ots:DEFINEDLAYOUT>
    </ots:LINK>

    <ots:LINK ID="CD" NODESTART="C" NODEEND="D" TYPE="HIGHWAY">
      <ots:STRAIGHT />
      <ots:DEFINEDLAYOUT>2LaneHighway</ots:DEFINEDLAYOUT>
    </ots:LINK>

    <ots:LINK ID="DE" NODESTART="D" NODEEND="E" TYPE="HIGHWAY">
      <ots:STRAIGHT />
      <ots:DEFINEDLAYOUT>2LaneHighway</ots:DEFINEDLAYOUT>
    </ots:LINK>

    <ots:LINK ID="FF2" NODESTART="F" NODEEND="F2" TYPE="LOCAL">
      <ots:STRAIGHT />
      <ots:DEFINEDLAYOUT>1LaneLocal</ots:DEFINEDLAYOUT>
    </ots:LINK>

    <ots:LINK ID="F2B" NODESTART="F2" NODEEND="B" TYPE="HIGHWAY" OFFSETEND="-3.6m">
      <ots:BEZIER />
      <ots:DEFINEDLAYOUT>1LaneHighway</ots:DEFINEDLAYOUT>
    </ots:LINK>

  </ots:NETWORK>

  <ots:NETWORKDEMAND>
    <ots:SINK LINK="DE" LANE="FORWARD1" DIRECTION="DIR_PLUS" POSITION="END-50m" />
    <ots:SINK LINK="DE" LANE="FORWARD2" DIRECTION="DIR_PLUS" POSITION="END-50m" />
  </ots:NETWORKDEMAND>

  <ots:SCENARIO></ots:SCENARIO>

  <ots:RUN>
    <ots:RUNLENGTH>1h</ots:RUNLENGTH>
  </ots:RUN>

</ots:OTS>